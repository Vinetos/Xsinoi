{
  "name": "Xsinoi",
  "version": "0.3.0",
  "short_name": "Xsinoi",
  "author": "Vinetos",
  "description": "the most popular dark theme extension for IonisX",
  "manifest_version": 2,
  "icons": {
    "512": "./assets/icons/icon4.png"
  },
  "content_scripts": [
    {
      "css": [
        "src/acdc.css",
        "src/cri.css",
        "src/debugpro.css",
        "src/ionisx.css",
        "src/pegasus.css"
      ],
      "matches": [
        "https://ionisx.com/*",
        "https://courses.ionisx.com/*",
        "https://*.helvetius.net/pegasus/*",
        "https://acdc.epita.fr/*",
        "http://cri.epita.fr/*",
        "http://www.debug-pro.com/*"
      ],
      "run_at": "document_start"
    }
  ]
}
